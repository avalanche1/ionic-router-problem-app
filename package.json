{
  "name": "internet-billing-app",
  "description": "Internet Billing App for Ferhat Telekom",
  "version": "0.1",
  "private": true,
  "scripts": {
    "start": "TSC_COMPILE_ON_ERROR=true DISABLE_ESLINT_PLUGIN=true ionic serve",
    "test": "react-scripts test",
    "check-phone-connected": "adb devices -l",
    "run-on-phone": "TSC_COMPILE_ON_ERROR=true ionic cap run android --livereload --external",
    "build-splash-screen-and-logo": "npx cordova-res android --skip-config --copy",
    "build-app": "ionic build --platform=android && npx cap copy android",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@ionic/react": "^5.6.8",
    "@ionic/react-router": "^5.6.8",
    "ionicons": "^5.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 10 chrome version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
